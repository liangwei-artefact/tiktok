+    const {accountDetailRequest} =  await req.json();
+    // console.log(JSON.stringify(accountDetailRequest))
+    const {accountName,dt,recentDays} = accountDetailRequest
+    const searchBody = []
+    // build searchBody
+    if (accountName) {
+        searchBody.push({
+            match:{account_name:accountName}
+        })
+    } 
